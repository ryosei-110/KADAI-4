<%= render 'un', book: @book, abc: @user %>

<div class="col-md-8 offset-md-1">
      <h2>Books</h2>
      <table class="table table-hover table-inverse">
       <thead>
         <tr>
           <th>image</th>
           <th>name</th>
           <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= image_tag book.user.get_profile_image(100,100) %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", edit_user_url_path %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>